{
  "name": "tide",
  "version": "1.0.0-dev.15",
  "registry": "github",
  "description": "Tide is a rapid application development framework for quickly building client-side applications with React and MobX.",
  "main": "dist/index.js",
  "web": "dist/index.js",
  "author": "Kay Sackey",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/KaySackey/tide.git"
  },
  "directories": {
    "lib": "src",
    "doc": "docs"
  },
  "homepage": "https://github.com/KaySackey/tide",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "transform-decorators-legacy",
          "transform-runtime"
        ]
      }
    }
  },
  "devDependencies": {
    "typescript": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "mobx": "3.x",
    "mobx-react": "4.x",
    "prop-types": "^15.5.10"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "@types/crossroads": "^0.0.29",
    "@types/history": "^4.6.0",
    "@types/react": "^15.0.34",
    "@types/signals": "^0.0.17",
    "crossroads": "^0.12.2",
    "history": "^4.0.0",
    "mobx-remotedev": "^0.2.8",
    "prop-types": "^15.5.10",
    "serializr": "^1.1.6"
  }
}
